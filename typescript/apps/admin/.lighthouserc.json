{
  "ci": {
    "collect": {
      "numberOfRuns": 1,
      "staticDistDir": "./dist",
      "url": [
        "http://localhost:3000/",
        "http://localhost:3000/sample/form",
        "http://localhost:3000/sample/separated-form"
      ],
      "settings": {
        "preset": "desktop"
      }
    },
    "assert": {
      "preset": "lighthouse:no-pwa",
      "assertions": {
        "offscreen-images": "off",
        "uses-webp-images": "off",
        "uses-http2": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
