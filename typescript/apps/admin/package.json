{
  "name": "@monorepo/admin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:nuxt": "nuxt",
    "dev:path": "pathpida --watch",
    "build": "pathpida && nuxt-ts build",
    "start": "nuxt-ts start",
    "export": "nuxt-ts export",
    "serve": "nuxt-ts serve",
    "lint": "run-p lint:*",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "hygen": "hygen",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "prettier --write",
      "eslint"
    ],
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/preset-env": "7.15.8",
    "@monorepo/fast-form": "./libs/fast-form",
    "@monorepo/proto": "./libs/proto",
    "@monorepo/util-functions": "./libs/util-functions",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxt/typescript-runtime": "2.1.0",
    "@nuxtjs/auth": "4.9.1",
    "@nuxtjs/axios": "5.13.6",
    "@types/uuid": "8.3.3",
    "@types/jest": "27.0.2",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "@vue/test-utils": "1.2.2",
    "babel-core": "6.26.3",
    "camelcase-keys": "6.2.2",
    "eslint-plugin-github": "4.3.5",
    "eslint-plugin-typescript": "0.14.0",
    "hygen": "6.1.0",
    "npm-run-all": "4.1.5",
    "jest": "27.3.1",
    "nuxt": "2.15.8",
    "nuxt-typed-vuex": "0.2.0",
    "pathpida": "0.17.0",
    "sass": "1.38.2",
    "snakecase-keys": "3.2.1",
    "ts-jest": "27.0.5",
    "ts-loader": "8.2.0",
    "ts-node": "10.2.1",
    "typed-vuex": "0.2.0",
    "typescript": "4.4.2",
    "vee-validate": "3.4.14",
    "uuid": "8.3.2",
    "vue-class-component": "7.2.6",
    "vue-eslint-parser": "7.10.0",
    "vue-jest": "3.0.7",
    "vue-property-decorator": "9.1.2",
    "vuex-module-decorators": "1.0.1"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/eslint-config": "3.1.0",
    "@nuxtjs/eslint-config-typescript": "2.1.0",
    "@nuxtjs/eslint-module": "2.0.0",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@nuxtjs/vuetify": "1.12.3",
    "@types/nuxtjs__auth": "4.8.8",
    "cypress": "6.9.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-nuxt": "1.0.0",
    "eslint-plugin-prettier": "3.4.1",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.4.1",
    "sass-loader": "7.3.1",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0"
  }
