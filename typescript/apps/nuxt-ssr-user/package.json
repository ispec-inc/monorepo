    "cypress": "6.9.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-nuxt": "1.0.0",
    "eslint-plugin-prettier": "3.4.1",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "node-sass": "4.14.1",
    "prettier": "2.3.2",
    "sass-loader": "7.3.1",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0"
  }
