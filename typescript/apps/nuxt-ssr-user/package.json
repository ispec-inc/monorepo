{
  "name": "@monorepo/nuxt-ssr-user",
  "description": "nuxt-ssr-user app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "export": "nuxt-ts export",
