{
  "name": "@monorepo/nuxt-apollo-admin",
  "version": "1.0.0",
  "packageManager": "yarn@1.22.17",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:nuxt": "nuxt",
    "dev:path": "pathpida --watch",
    "build": "pathpida && nuxt-ts build",
    "start": "nuxt-ts start",
    "export": "nuxt-ts export",
    "serve": "nuxt-ts serve",
    "lint": "run-p lint:*",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "hygen": "hygen",
    "test": "jest",
    "prepare": "cd ../../../ && husky install typescript/apps/nuxt-apollo-admin/.husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,vue}": [
      "eslint --fix"
    ],
    "**/*.{scss,vue}": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.5.10",
    "@babel/preset-env": "7.20.2",
    "@monorepo/fast-form": "./libs/fast-form",
    "@monorepo/util-functions": "./libs/util-functions",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxt/typescript-runtime": "2.1.0",
    "@nuxtjs/apollo": "4.0.1-rc.5",
    "@nuxtjs/auth": "4.9.1",
    "@nuxtjs/axios": "5.13.6",
    "@types/jest": "27.5.2",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "@vue/apollo-option": "4.0.0-alpha.20",
    "@vue/test-utils": "1.3.3",
    "babel-core": "6.26.3",
    "camelcase-keys": "6.2.2",
    "eslint-plugin-typescript": "0.14.0",
    "graphql": "16.3.0",
    "graphql-tag": "2.12.6",
    "husky": "7.0.4",
    "hygen": "6.2.8",
    "jest": "27.5.1",
    "npm-run-all": "4.1.5",
    "nuxt": "2.15.8",
    "nuxt-property-decorator": "2.9.1",
    "nuxt-typed-vuex": "0.3.1",
    "pathpida": "0.17.0",
    "sass": "1.56.1",
    "snakecase-keys": "3.2.1",
    "ts-jest": "27.1.5",
    "ts-loader": "8.4.0",
    "ts-node": "10.2.1",
    "typed-vuex": "0.3.1",
    "typescript": "4.4.2",
    "uuid": "8.3.2",
    "vee-validate": "3.4.14",
    "vue-class-component": "7.2.6",
    "vue-eslint-parser": "7.11.0",
    "vue-jest": "3.0.7",
    "vue-property-decorator": "9.1.2",
    "vuex-module-decorators": "1.0.1"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/eslint-config": "3.1.0",
    "@nuxtjs/eslint-config-typescript": "2.1.0",
    "@nuxtjs/eslint-module": "2.0.0",
    "@nuxtjs/stylelint-module": "4.1.0",
    "@nuxtjs/vuetify": "1.12.3",
    "@types/nuxtjs__auth": "4.8.8",
    "cypress": "6.9.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-nuxt": "1.0.0",
    "eslint-plugin-prettier": "3.4.1",
    "lint-staged": "10.5.4",
    "sass-loader": "7.3.1",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0"
  }
}
